            <a is="cursor-link" data-hint="NodeJS" cursor-class="dot light" class="techno t-node"></a>
        
            <a is="cursor-link" data-hint="Socket.io" cursor-class="dot light" class="techno t-socket-io"></a>
        
            <a is="cursor-link" data-hint="PHP" cursor-class="dot light" class="techno t-php"></a>
        
            <a is="cursor-link" data-hint="Python" cursor-class="dot light" class="techno t-python"></a>
        
            <a is="cursor-link" data-hint="Contrôleur de version Git" cursor-class="dot light" class="techno t-git"></a>
        
            <a is="cursor-link" data-hint="MySQL / MariaDB" cursor-class="dot light" class="techno t-mysql"></a>
        
