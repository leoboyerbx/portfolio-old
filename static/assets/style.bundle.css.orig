.cross {
  position: relative;
  width: 20px;
  height: 20px; }
  .cross .x-bar {
    position: absolute;
    height: 3px;
    left: 0;
    right: 0;
    border-radius: 10px;
    transform: translateY(10px) rotate(-45deg);
    overflow: hidden; }
    .cross .x-bar::before {
      content: '';
      position: absolute;
      width: 0;
      transition: all .2s .2s;
      height: 100%;
      border-radius: 10px;
      top: 0;
      left: 0;
      background-color: #fff;
      animation: trace .3s both .6s; }
    .cross .x-bar:last-child::before {
      animation-delay: .8s; }

@keyframes trace {
  to {
    width: 100%; } }
    .cross .x-bar:last-child {
      transform: translateY(10px) rotate(45deg); }

.arrow {
  position: relative;
  width: 20px;
  height: 20px; }
  .arrow.right {
    transform: scale(-1, 1); }
  .arrow .arrow-bars {
    transition: all .2s .1s;
    transform: translate(50%, 10px); }
    .arrow .arrow-bars .a-bar {
      position: absolute;
      height: 3px;
      left: 0;
      width: 60%;
      border-radius: 10px;
      transform: rotate(-45deg) translateY(1.5px);
      transform-origin: left center;
      overflow: hidden; }
      .arrow .arrow-bars .a-bar::before {
        content: '';
        position: absolute;
        width: 0;
        transition: all .2s .2s;
        height: 100%;
        border-radius: 10px;
        top: 0;
        left: 0;
        background-color: #fff;
        animation: trace .3s both .6s; }
      .arrow .arrow-bars .a-bar:last-child::before {
        animation-delay: .8s; }

@keyframes trace {
  to {
    width: 100%; } }
      .arrow .arrow-bars .a-bar:last-child {
        transform: rotate(45deg) translateY(-1.5px); }
  .arrow .h-bar {
    position: absolute;
    height: 3px;
    width: 80%;
    left: 15%;
    border-radius: 10px;
    transform: translateY(10px); }
    .arrow .h-bar::before {
      content: '';
      position: absolute;
      width: 0;
      transition: all .2s;
      height: 100%;
      border-radius: 10px;
      top: 0;
      right: 0;
      background-color: #fff; }
  .arrow:hover .arrow-bars {
    transform: translate(0, 10px);
    transition-delay: 0s; }
  .arrow:hover .h-bar::before {
    width: 100%;
    transition: all .2s .1s; }

