#menu-button {
  display: block;
  cursor: pointer;
  z-index: 210; }
  #menu-button #menu-button-hint {
    display: block;
    transition: all 0.3s cubic-bezier(0.42, 0, 0.11, 0.98); }
  #menu-button #menu-button-icon {
    z-index: 200;
    position: fixed;
    width: 40px;
    height: 40px;
    left: 20px;
    top: 20px;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    transform: scale(0.7); }
    #menu-button #menu-button-icon::before {
      content: '';
      background-color: #fff;
      position: absolute;
      width: 100%;
      height: 100%;
      border-radius: 50%;
      transform: scale(2);
      transition: all .2s .5s; }
    #menu-button #menu-button-icon .h-bar {
      position: relative;
      height: 4px;
      width: 100%; }
    #menu-button #menu-button-icon .h-bar::after, #menu-button #menu-button-icon .h-bar::before {
      content: '';
      position: absolute;
      background-color: #101010;
      border-radius: 10px;
      height: 100%;
      transition: all 0.3s cubic-bezier(0.42, 0, 0.11, 0.98); }
    #menu-button #menu-button-icon .h-bar:first-child::before {
      transform-origin: left center; }
    #menu-button #menu-button-icon .h-bar:nth-child(1)::before {
      left: 0;
      right: 25%; }
    #menu-button #menu-button-icon .h-bar:nth-child(1)::after {
      left: 90%;
      right: 0; }
    #menu-button #menu-button-icon .h-bar:nth-child(2)::before {
      left: 0;
      right: 90%; }
    #menu-button #menu-button-icon .h-bar:nth-child(2)::after {
      left: 25%;
      right: 0; }
    #menu-button #menu-button-icon .h-bar:nth-child(3)::before {
      left: 0;
      right: 50%; }
    #menu-button #menu-button-icon .h-bar:nth-child(3)::after {
      left: 65%;
      right: 0; }
    #menu-button #menu-button-icon .x-bar {
      position: absolute;
      height: 4px;
      width: 100%;
      border-radius: 5px;
      overflow: hidden; }
      #menu-button #menu-button-icon .x-bar::before, #menu-button #menu-button-icon .x-bar:nth-child(4)::after {
        position: absolute;
        content: '';
        width: 0;
        height: 100%;
        border-radius: 5px;
        left: 0;
        top: 0;
        background-color: #101010;
        transition: all 0.3s cubic-bezier(0.42, 0, 0.11, 0.98); }
      #menu-button #menu-button-icon .x-bar:nth-child(4)::after {
        right: 0;
        left: auto; }
      #menu-button #menu-button-icon .x-bar:nth-child(4) {
        transform: rotate(45deg) scaleX(1.1); }
      #menu-button #menu-button-icon .x-bar:last-child {
        transform: rotate(-45deg) scaleX(1.1); }
  #menu-button:hover #menu-button-icon .h-bar:nth-child(1)::before {
