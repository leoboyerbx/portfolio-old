#cursor-follower {
  position: absolute;
  z-index: 8000;
  top: -100px;
  left: 0;
  box-sizing: border-box;
  padding: 0;
  width: 40px;
  height: 40px;
  transition: all .2s ease;
  transform: translate(-100%, -100%); }
  #cursor-follower .round-cursor {
    transition: all .2s ease;
    position: absolute;
    border-radius: 50%;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    transform: scale(0.82);
    border: solid #fce14b 3px; }
    #cursor-follower .round-cursor::before {
      content: '';
      position: absolute;
      top: 115%;
      left: 75%;
      width: 0;
      height: 5px;
      border-radius: 4px;
      background-color: #fce14b;
      transform: rotate(45deg);
      transition: all .2s ease; }
  #cursor-follower .plus-cursor {
    transition: all .2s ease;
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    transform: scale(0.3);
    opacity: 0; }
    #cursor-follower .plus-cursor::before {
      content: '';
      position: absolute;
      height: 100%;
      width: 5px;
      border-radius: 10px;
      top: 0;
      left: 50%;
      transform: translateX(-50%);
      background-color: #323739; }
    #cursor-follower .plus-cursor::after {
      content: '';
      position: absolute;
      width: 100%;
      height: 5px;
      border-radius: 10px;
      left: 0;
      top: 50%;
      transform: translateY(-50%);
      background-color: #323739; }
  #cursor-follower .plus-cursor-info {
    content: '';
    position: absolute;
    text-align: left;
    width: 200%;
    top: 110%;
    left: 50%;
    color: #fce14b;
    font-family: 'Raleway Black', sans-serif;
    font-size: 170%;
    letter-spacing: 10px;
    opacity: 0;
    transition: all .2s; }
  #cursor-follower .restart-cursor {
    transition: all .2s ease;
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    transform: scale(0.5) rotate(-180deg);
    opacity: 0;
    font-size: 22px; }
  #cursor-follower.hover1 .round-cursor {
    transform: scale(0.5);
    background-color: #fce14b; }
  #cursor-follower.dot .round-cursor {
    transform: scale(0.2);
    background-color: #fce14b; }
  #cursor-follower.dark .round-cursor {
    background-color: #323739;
    border-color: #323739; }
  #cursor-follower.light .round-cursor {
    background-color: #f5f7fc;
    border-color: #f5f7fc; }
  #cursor-follower.soften {
    opacity: .8; }
  #cursor-follower.zoom .round-cursor {
    border-width: 5px;
    transform: scale(0.5) translate(-50%, -50%); }
    #cursor-follower.zoom .round-cursor::before {
      width: 80%; }
  #cursor-follower.hidden {
    transition: all .3s;
    opacity: 0; }
  #cursor-follower.hover-plus .round-cursor {
    transform: scale(1.4);
    background-color: #fce14b; }
  #cursor-follower.hover-plus .plus-cursor {
    transform: scale(0.5);
    opacity: 1; }
  #cursor-follower.hover-restart .round-cursor {
    transform: scale(1.2);
    background-color: #fce14b; }
  #cursor-follower.hover-restart .restart-cursor {
    transition: all .4s ease;
    transform: scale(0.6);
    opacity: 1; }
  #cursor-follower .progress-cursor {
    position: absolute;
    top: 0;
    left: 0;
    width: 40px;
    height: 40px;
    transform: rotate(-90deg);
    stroke: #fce14b;
    transition: all .3s;
    opacity: 0; }
    #cursor-follower .progress-cursor circle {
      stroke-dasharray: 130;
      stroke-dashoffset: 0;
      animation: dash 2s infinite; }

@keyframes dash {
  0% {
    stroke-dashoffset: 130; }
  50% {
    stroke-dashoffset: 0; }
  100% {
    stroke-dashoffset: -130; } }
  #cursor-follower.ext-page {
    stroke: #323739; }
  #cursor-follower.loading .round-cursor, #cursor-follower.loading .plus-cursor, #cursor-follower.loading .cross-cursor, #cursor-follower.loading .restart-cursor {
    opacity: 0 !important; }
  #cursor-follower.loading .progress-cursor {
